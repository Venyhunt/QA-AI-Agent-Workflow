      "parameters": {
        "jsCode": "const allTestCases = [];\n\nfor (const item of items) {\n  const parsed = JSON.parse(item.json.output);\n  \n  if (parsed.test_cases && parsed.test_cases.length > 0) {\n    allTestCases.push(...parsed.test_cases);\n  }\n}\n\nreturn [\n  {\n    json: {\n      test_cases: allTestCases\n    }\n  }\n];"
      },
